<Login>:
    username: username
    password: password
    FloatLayout:

        Image:
            source: 'test.jpg'
            size: self.texture_size
            allow_stretch: True
            keep_ratio: False

        CustomLogIn:
            text: 'Username:'
            pos_hint:{"x":0.23, "y":0.7}

        CustomLogInTI:
            id: username
            pos_hint:{"x":0.43, "y":0.7}

        CustomLogIn:
            text: 'Password:'
            pos_hint:{"x":0.23, "y":0.63}

        CustomLogInTI:
            id: password
            pos_hint:{"x":0.43, "y":0.63}
            password: True

        Button:
            text: "Login"
            size_hint:(0.15, 0.05)
            pos_hint:{"x":0.41, "y":0.55}
            background_color: (0,1,0,1)
            on_release: root.login_btn()

        Label:
            text: "Don't have an account? Sign up for free!"
            bold: True
            underline: True
            color: [0,0,0,1]
            size_hint:(0.5, 0.05)
            pos_hint:{"x":0.23, "y":0.4}
            background_color: (77/255,227/255,102/255,1)
            canvas.before:
                Color:
                    rgba: self.background_color
                Rectangle:
                    size: self.size
                    pos: self.pos

        Button:
            text: "Sign up"
            size_hint:(0.15, 0.05)
            pos_hint:{"x":0.41, "y":0.31}
            color: [0,0,0,1]
            background_normal: ''
            background_color: (219/255,210/255,42/255,1)
            on_release:
                root.manager.current = 'create'
                root.manager.transition.direction = 'left'

        Button:
            text: "fast"
            size_hint:(0.15, 0.05)
            pos_hint:{"x":0.41, "y":0.21}
            background_normal: ''
            background_color: (187/255,63/255,224/255,1)
            on_release:
                root.manager.current = 'main'
                root.manager.transition.direction = 'left'

